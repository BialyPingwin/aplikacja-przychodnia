        <Button x:Name="Button_Logout" Content="Wyloguj" HorizontalAlignment="Left" Margin="59,211,0,0" VerticalAlignment="Top" Width="184" Height="44" FontSize="20" Background="#FF404040" Foreground="White" BorderBrush="White" BorderThickness="3" Click="Button_Logout_Click"/>

    </Grid>
